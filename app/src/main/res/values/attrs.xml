<?xml version="1.0" encoding="utf-8"?>
<resources>
	<!--	https://stackoverflow.com/questions/60590968/set-materialbutton-style-to-textbutton-programmatically-->
	<attr name="materialButtonTextStyle" format="reference" />

	<attr name="alphaEmphasisHigh" format="float" />
	<attr name="alphaEmphasisMedium" format="float" />
	<attr name="alphaEmphasisDisabled" format="float" />

	<declare-styleable name="AdapterIndicator">
		<attr name="dotCount" format="integer" />
		<attr name="fadingDotCount" format="integer" />
		<attr name="dotRadius" format="dimension" />
		<attr name="selectedDotRadius" format="dimension" />
		<attr name="dotColor" format="color" />
		<attr name="selectedDotColor" format="color" />
		<attr name="dotSeparation" format="dimension" />
		<attr name="supportRTL" format="boolean" />
		<attr name="verticalSupport" format="boolean" />
	</declare-styleable>
</resources>